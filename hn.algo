PROCEDURE orthogonality()
VAR
v1 :STRING[2]
v2:STRING[2]
ang:INTEGER
BEGIN
    IF (ang=90 ) THEN
    write(v1 , v2 "are orthogonal")
    else 
    write(v1,v2 "are not orthogonal")
        
    END_IF
END






ALGORITHM 

VAR
    v1:STRING[2]
    v2:STRING[2]
    ang:INTEGER
    ortho:STRING
BEGIN
    write("give the first vector")
    read(v1)
    write("give the second vector")
    read(v2)
    write("give the angle")
    read(ang)
    ortho:=orthogonality
    write(ortho)

END